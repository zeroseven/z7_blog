{
  "name": "zeroseven/z7-blog",
  "license": "GPL-2.0-only",
  "type": "typo3-cms-extension",
  "description": "Another blog-system for TYPO3",
  "authors": [
    {
      "name": "zeroseven design studios GmbH",
      "email": "typo3@zeroseven.de",
      "homepage": "https://www.zeroseven.de/",
      "role": "Company"
    },
    {
      "name": "Raphael Thanner",
      "role": "Developer"
    },
    {
      "name": "Sebastian Rosskopf",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^9.5 || ^10.4 || ^11.2",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Zeroseven\\Z7Blog\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "z7_blog"
    }
  },
  "replace": {
    "zeroseven/z7-blog": "self.version"
  }
}
